{
	"BASE_DIR":"~/projects/sycamore_projects",
	"board":"sycamore1",
	"PROJECT_NAME":"example_project",
	"TEMPLATE":"wishbone_template.json",
	"INTERFACE":{
		"filename":"ft_host_interface.v",
		"bind":{
			"i_ftdi_clk":{
				"port":"i_ftdi_clk",
				"direction":"input"
			},
			"io_ftdi_data[7:0]":{
				"port":"d[7:0]",
				"direction":"inout"
			},
			"i_ftdi_txe_n":{
				"port":"txe_n",
				"direction":"input"
			},
			"o_ftdi_wr_n":{
				"port":"wr_n",
				"direction":"output"
			},
			"i_ftdi_rde_n":{
				"port":"rxe_n",
				"direction":"input"
			},
			"o_ftdi_rd_n":{
				"port":"rd_n",
				"direction":"output"
			},
			"o_ftdi_oe_n":{
				"port":"oe_n",
				"direction":"output"
			},
			"o_ftdi_siwu":{
				"port":"siwua",
				"direction":"output"
			},
			"o_ftdi_suspend_n":{
				"port":"suspend_n",
				"direction":"input"
			}
		}
	},
	"SLAVES":{
		"gpio1":{
			"filename":"wb_gpio.v",
			"bind":{
				"gpio_out[1:0]":{
					"port":"led[1:0]",
					"direction":"output"
				},
				"gpio_in[0]":{
					"port":"button",
					"direction":"input"
				}
			}
		}
	},

	"bind": {
	},
	"constraint_files":[
	]

}
