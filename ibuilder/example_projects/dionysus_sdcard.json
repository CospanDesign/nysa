{
	"BASE_DIR":"~/projects/sycamore_projects",
	"board":"dionysus",
	"PROJECT_NAME":"example_project",
	"TEMPLATE":"wishbone_template.json",
	"INTERFACE":{
		"filename":"ft_master_interface.v",
		"bind":{
			"ftdi_clk":{
				"port":"ftdi_clk",
				"direction":"input"
			},
			"ftdi_data[7:0]":{
				"port":"d[7:0]",
				"direction":"inout"
			},
			"ftdi_txe_n":{
				"port":"txe_n",
				"direction":"input"
			},
			"ftdi_wr_n":{
				"port":"wr_n",
				"direction":"output"
			},
			"ftdi_rde_n":{
				"port":"rxe_n",
				"direction":"input"
			},
			"ftdi_rd_n":{
				"port":"rd_n",
				"direction":"output"
			},
			"ftdi_oe_n":{
				"port":"oe_n",
				"direction":"output"
			},
			"ftdi_siwu":{
				"port":"siwua",
				"direction":"output"
			},
			"ftdi_suspend_n":{
				"port":"suspend_n",
				"direction":"input"
			}
		}
	},
	"SLAVES":{
		"gpio1":{
			"filename":"wb_gpio.v",
			"bind":{
				"gpio_out[1:0]":{
					"port":"led[1:0]",
					"direction":"output"
				},
 				"gpio_in[1:0]":{
					"port":"button[1:0]",
					"direction":"input"
				}
			}
  	},
    "sdcard":{
      "filename":"wb_spi.v",
      "bind":{
        "ss_pad_o[0]":{
          "port":"s10",
          "direction":"output"
        },
        "sclk_pad_o":{
          "port":"s13",
          "direction":"output"
        },
        "mosi_pad_o":{
          "port":"s11",
          "direction":"output"
        },
        "miso_pad_i":{
          "port":"s12",
          "direction":"input"
        }
      }
    }
  },
  "MEMORY":{
		"mem1":{
			"filename":"wb_sdram.v",
			"bind":{
	
				"sdram_clk":{
					"port":"sdram_clk",
					"direction":"output"
				},
				"sdram_cke":{
					"port":"cke",
					"direction":"output"
				},
				"sdram_cs_n":{
					"port":"cs_n",
					"direction":"output"
				},
				"sdram_ras":{
					"port":"ras",
					"direction":"output"
				},
				"sdram_cas":{
					"port":"cas",
					"direction":"output"
				},
				"sdram_we":{
					"port":"we",
					"direction":"output"
				},
				"sdram_bank[1:0]":{
					"port":"ba[1:0]",
					"direction":"output"
				},
				"sdram_addr[11:0]":{
					"port":"a[11:0]",
					"direction":"output"
				},
				"sdram_data[15:0]":{
					"port":"dq[15:0]",
					"direction":"inout"
				},
				"sdram_data_mask[1]":{
					"port":"dqmh",
					"direction":"output"
				},
				"sdram_data_mask[0]":{
					"port":"dqml",
					"direction":"output"
				}

			}
		}
	},

	"bind": {
	},
	"constraint_files":[
	]

}
