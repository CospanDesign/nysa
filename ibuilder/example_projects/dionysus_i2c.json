{
	"BASE_DIR":"~/projects/sycamore_projects",
	"board":"dionysus",
	"PROJECT_NAME":"example_project",
	"TEMPLATE":"wishbone_template.json",
	"INTERFACE":{
		"filename":"ft_master_interface.v",
		"bind":{
			"i_ftdi_clk":{
				"port":"i_ftdi_clk",
				"direction":"input"
			},
			"io_ftdi_data[7:0]":{
				"port":"d[7:0]",
				"direction":"inout"
			},
			"i_ftdi_txe_n":{
				"port":"txe_n",
				"direction":"input"
			},
			"o_ftdi_wr_n":{
				"port":"wr_n",
				"direction":"output"
			},
			"i_ftdi_rde_n":{
				"port":"rxe_n",
				"direction":"input"
			},
			"o_ftdi_rd_n":{
				"port":"rd_n",
				"direction":"output"
			},
			"o_ftdi_oe_n":{
				"port":"oe_n",
				"direction":"output"
			},
			"o_ftdi_siwu":{
				"port":"siwua",
				"direction":"output"
			},
			"o_ftdi_suspend_n":{
				"port":"suspend_n",
				"direction":"input"
			}
		}
	},
	"SLAVES":{
		"gpio1":{
			"filename":"wb_gpio.v",
			"bind":{
				"gpio_out[1:0]":{
					"port":"led[1:0]",
					"direction":"output"
				},
        "gpio_out[2]":{
          "port":"pmoda7",
          "direction":"output"
        },
        "gpio_out[3]":{
          "port":"pmoda8",
          "direction":"output"
        },
        "gpio_out[4]":{
          "port":"pmoda9",
          "direction":"output"
        },
        "gpio_out[5]":{
          "port":"pmoda10",
          "direction":"output"
        },
				"gpio_in[7:6]":{
					"port":"button[1:0]",
					"direction":"input"
				}
			}
  	},
    "i2c1":{
      "filename":"wb_i2c.v",
      "bind":{
        "scl":{
          "port":"s0",
          "direction":"inout"
        },
        "sda":{
          "port":"s1",
          "direction":"inout"
        }
      }
    }
  },
  "MEMORY":{
		"mem1":{
			"filename":"wb_sdram.v",
			"bind":{
	
				"sdram_clk":{
					"port":"sdram_clk",
					"direction":"output"
				},
				"sdram_cke":{
					"port":"cke",
					"direction":"output"
				},
				"sdram_cs_n":{
					"port":"cs_n",
					"direction":"output"
				},
				"sdram_ras":{
					"port":"ras",
					"direction":"output"
				},
				"sdram_cas":{
					"port":"cas",
					"direction":"output"
				},
				"sdram_we":{
					"port":"we",
					"direction":"output"
				},
				"sdram_bank[1:0]":{
					"port":"ba[1:0]",
					"direction":"output"
				},
				"sdram_addr[11:0]":{
					"port":"a[11:0]",
					"direction":"output"
				},
				"sdram_data[15:0]":{
					"port":"dq[15:0]",
					"direction":"inout"
				},
				"sdram_data_mask[1]":{
					"port":"dqmh",
					"direction":"output"
				},
				"sdram_data_mask[0]":{
					"port":"dqml",
					"direction":"output"
				}

			}
		}
	},

	"bind": {
	},
	"constraint_files":[
	]

}
